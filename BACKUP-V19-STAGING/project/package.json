{
  "name": "goldeouro-backend",
  "version": "1.2.0",
  "description": "Backend do Gol de Ouro - Sistema de Apostas",
  "main": "server-fly.js",
  "scripts": {
    "start": "node server-fly.js",
    "dev": "nodemon server-fly.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:rls": "vitest run src/tests/rls.policies.test.js",
    "test:concurrency": "vitest run src/tests/concurrency.fila.test.js",
    "test:migration": "vitest run src/tests/migration.integration.test.js",
    "test:smoke": "vitest run src/tests/smoke.test.js",
    "report": "node scripts/generate-integrity-report-pdf.cjs",
    "test:e2e:prod": "node scripts/e2e/e2e-production-v9-corrigido.js",
    "test:data-testid": "node scripts/e2e/validate-data-testid.js",
    "migrate:v19": "bash src/migrations/apply_migration.sh",
    "migrate:memory": "node src/scripts/migrate_memory_lotes_to_db.js",
    "verify:backup": "node src/scripts/verify_backup_and_proceed.js",
    "backup:pre-migration": "bash src/scripts/backup_before_migration.sh",
    "archive:obsolete": "bash src/scripts/archive_obsolete_code.sh",
    "audit:check": "node src/scripts/auditoria_check.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.7",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.3.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.10.0",
    "nodemailer": "^6.9.8",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "jest": "^30.2.0",
    "lighthouse": "^12.8.2",
    "nodemon": "^3.0.2",
    "pg": "^8.16.3",
    "prom-client": "^15.0.0",
    "puppeteer": "^24.31.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "backend",
    "api",
    "gol-de-ouro",
    "apostas",
    "supabase"
  ],
  "author": "Gol de Ouro Team",
  "license": "MIT"
}
