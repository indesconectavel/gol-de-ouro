{
  "name": "goldeouro-player",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/inject-build-info.cjs || echo 'Build info script skipped'",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test",
    "health:check": "node scripts/network-smoke.js",
    "validate": "node validate-player-system.cjs",
    "rollback": "node rollback-player-to-approved.cjs",
    "status": "node -e \"console.log('Sistema Jogador Aprovado v1.1.1 - Funcionando Perfeitamente')\"",
    "audit:pre-deploy": "cd ../mcp-system && node audit-simple.js",
    "deploy:safe": "npm run audit:pre-deploy && npm run build && npx vercel --prod",
    "audit:full": "cd ../mcp-system && node audit-simple.js"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^30.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.9.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-window": "^7.3.0"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "tar": "^7.5.7"
  }
